{
	"name": "@pouchlab/liteq",
	"version": "1.1.0",
	"description": "liteq is a key value document single database engine for js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pouchlabs/pouchlite.git"
	},
	"bugs": {
		"url": "https://github.com/pouchlabs/pouchlite/issues"
	},
	"author": {
		"name": "pouchlabs"
	},
	"contributors": [
		{
			"name": "pouchlabs"
		}
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "node server.js",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "npm run test:integration && npm run test:unit",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"dev:server": "node --watch ./src/lib/demo.js"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"!dist/lib/demo.js"
	],
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"chokidar": "^3.6.0",
		"console-clear": "^1.1.1",
		"detect-port": "^1.5.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"http-terminator": "^3.2.0",
		"polka": "^0.5.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.1.9",
		"sirv": "^2.0.4",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.11",
		"vitest": "^1.2.0"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"adm-zip": "^0.5.16",
		"deepmerge": "^4.3.1",
		"eventemitter3": "^5.0.1",
		"jimp": "^1.6.0",
		"lodash": "^4.17.21",
		"msgpack-lite": "^0.1.26",
		"nanoid": "^5.0.7"
	},
	"keywords": [
		"sync",
		"db",
		"database",
		"orm",
		"offline-first",
		"embeddable",
		"ultra fast",
		"blazing fast",
		"nodejs",
		"secure",
		"compressed",
		"storage",
		"cache",
		"ttl",
		"redis"
	]
}
